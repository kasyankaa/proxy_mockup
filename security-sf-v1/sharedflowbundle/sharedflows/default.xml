<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<SharedFlow name="default">
    <Step>
        <Name>KVM0-GetIPAddress</Name>
    </Step>
    <Step>
        <Name>JS-VerifyIPAddress</Name>
    </Step>
    <Step>
        <Condition>triggerError equals "true"</Condition>
        <Name>RF-DeniedIPAddress</Name>
    </Step>
    <Step>
        <Name>VAK-VerifyApiKey</Name>
    </Step>
    <Step>
        <Name>EV-AppRatelimitingVariables</Name>
    </Step>
    <Step>
        <Name>SA-30ps</Name>
    </Step>
    <Step>
        <Name>Q-5ps</Name>
    </Step>
    <Step>
        <Name>OAV2-VerifyAccessToken</Name>
    </Step>
    <Step>
        <Condition>(verifyapikey.Verify-Api-Key.client_id != apigee.client_id)</Condition>
        <Name>RF-StolenKey</Name>
    </Step>
</SharedFlow>